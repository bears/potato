server
{
	listen		80;
	server_name	potato.bears.home;

	location	/
	{
		root	D:/Project/php/potato/public;
		index	index.html;
	}

	gzip_static	on;
	gzip_types	text/plain text/css text/xml application/x-javascript;
}

server
{
	listen		80;
	server_name	ajaj.bears.home;

	location ~	/
	{
		include			fastcgi_params;
		fastcgi_pass	127.0.0.1:9000;
		fastcgi_param	SCRIPT_FILENAME D:/Project/php/potato/script/index.php;
	}
}